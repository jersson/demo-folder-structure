{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": [
    "plugin:react/recommended",
    "plugin:prettier/recommended",
    "google"
  ],
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true
    },
    "ecmaVersion": 12,
    "sourceType": "module"
  },
  "plugins": ["react", "@typescript-eslint"],
  "rules": {
    "quotes": "off",
    "object-curly-spacing": "off",
    "semi": "error",
    "require-jsdoc": "off",
    "operator-linebreak": "off",
    "indent": "off",
    "max-len": "off",
    "comma-dangle": "off"
  }
}
